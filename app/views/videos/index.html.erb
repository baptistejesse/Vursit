<h1>Feed</h1>

<div class="main" style="margin: 0 auto;position:relative;">
<% @videos.each do |video| %>

  
   <div id="vidds" style="margin:10px;float:left;padding:15px;-webkit-border-radius: 5px;
-moz-border-radius: 5px;
border-radius: 5px;"><h6 style="text-align:center;"><%= "#{ video.title}" %></h6><% if video.typical == "music" %>
<iframe src="https://embed.spotify.com/?uri=<%= video.viddy %>" width="300" height="380" frameborder="0" allowtransparency="true"></iframe>
<br/>
<% elsif video.typical == "video" %>
<a href="<%= video.viddy%>"</a>

<% elsif video.typical == "tweet" %>
<%= video.content.html_safe %>
<%end%>
 <%=link_to 'Show', video, :class => "btn " %> </div>
   
  

<% end %>

</div>
<br />


